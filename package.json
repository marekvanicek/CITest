{
  "name": "citest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^7.0.0-beta.54",
    "@storybook/addon-essentials": "^7.0.0-beta.54",
    "@storybook/addon-interactions": "^7.0.0-beta.54",
    "@storybook/addon-links": "^7.0.0-beta.54",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.54",
    "@storybook/vue3-vite": "^7.0.0-beta.54",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.0-beta.54",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  }
}
